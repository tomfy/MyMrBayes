begin mrbayes;
set autoclose=yes nowarn=yes;
set seed=12345;
set swapseed=13345;
execute ./famX.nex;
set precision=6;
lset rates=invgamma;
prset aamodelpr=fixed(wag);
mcmcp minpartfreq=0.02;
mcmcp allchains=yes;
mcmcp burninfrac=0.2;
mcmcp nchains=3;
mcmcp nchainsout=3;
mcmcp nswaps=1;
mcmcp nruns=2;
mcmcp temp=(1.0, 1.2, 1.5);
mcmcp samplefreq=50;
mcmcp printfreq=100;
mcmcp checkpoint=yes checkfreq=200;
mcmc append=yes ngen=4000;
sump;
sumt;
end;
